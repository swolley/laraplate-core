{
    "name": "swolley/laraplate-core",
    "description": "Core Module contains all the necessary functionalities for build up a new Laravel application.",
    "version": "1.3.0",
    "keywords": [
        "laravel",
        "laraplate",
        "core"
    ],
    "authors": [
        {
            "name": "Swolley",
            "email": "myentity@inwind.it"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-intl": "*",
        "ext-sockets": "*",
        "doctrine/dbal": "*",
        "laravel/fortify": "^1.21",
        "hedii/laravel-gelf-logger": "^9.0",
        "lab404/laravel-impersonate": "^1.7",
        "overtrue/laravel-versionable": "^5.2",
        "spatie/laravel-permission": "^6.4",
        "staudenmeir/laravel-adjacency-list": "^1.23",
        "laravel/socialite": "^5.15",
        "stephenlake/laravel-approval": "^1.1",
        "wotz/laravel-swagger-ui": "^1.0",
        "elasticsearch/elasticsearch": "^8.15",
        "theodo-group/llphant": "^0.8.8",
        "laravel/horizon": "^5.30",
        "laravel/octane": "^2.6",
        "awobaz/compoships": "^2.3"
    },
    "require-dev": {
        "mtrajano/laravel-swagger": "^0.6.4",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0",
        "mockery/mockery": "^1.6"
        
    },
    "scripts": {
        "version:major": "./scripts/version.sh major",
        "version:minor": "./scripts/version.sh minor",
        "version:patch": "./scripts/version.sh patch",
        "setup:hooks": "./scripts/setup-hooks.sh"
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": []
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Core\\": "app/",
            "Modules\\Core\\Database\\Factories\\": "database/factories/",
            "Modules\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Core\\Tests\\": "tests/"
        }
    }
}
