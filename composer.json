{
    "name": "swolley/laraplate-core",
    "description": "Core Module contains all the necessary functionalities for build up a new Laravel application.",
    "version": "v1.14.0",
    "keywords": [
        "laravel",
        "laraplate",
        "core"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Swolley",
            "email": "myentity@inwind.it"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-intl": "*",
        "ext-sockets": "*",
        "doctrine/dbal": "*",
        "laravel/framework": "^12.0",
        "laravel/fortify": "^1.25",
        "hedii/laravel-gelf-logger": "^10.0",
        "lab404/laravel-impersonate": "^1.7",
        "overtrue/laravel-versionable": "^5.4",
        "spatie/laravel-permission": "^6.16",
        "staudenmeir/laravel-adjacency-list": "^1.24",
        "laravel/socialite": "^5.18",
        "stephenlake/laravel-approval": "^1.1",
        "wotz/laravel-swagger-ui": "^1.2",
        "elasticsearch/elasticsearch": "^8.17",
        "theodo-group/llphant": "^0.9.7",
        "laravel/horizon": "^5.31",
        "awobaz/compoships": "^2.4",
        "babenkoivan/elastic-scout-driver": "^4.0",
        "typesense/typesense-php": "^5.0",
        "laravel/scout": "^10.14",
        "matanyadaev/laravel-eloquent-spatial": "^4.5",
        "babenkoivan/elastic-scout-driver-plus": "^5.1"
    },
    "require-dev": {
        "mtrajano/laravel-swagger": "^0.6.4"
    },
    "scripts": {
        "version:major": "./scripts/version.sh major",
        "version:minor": "./scripts/version.sh minor",
        "version:patch": "./scripts/version.sh patch",
        "setup:hooks": "./scripts/setup-hooks.sh"
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {

            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Core\\": "app/",
            "Modules\\Core\\Database\\Factories\\": "database/factories/",
            "Modules\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Core\\Tests\\": "tests/"
        }
    }
}
