{
    "name": "swolley/laraplate-core",
    "description": "Core Module contains all the necessary functionalities for build up a new Laravel application.",
    "version": "v1.23.0",
    "keywords": [
        "laravel",
        "laraplate",
        "core"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Swolley",
            "email": "myentity@inwind.it"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-intl": "*",
        "ext-sockets": "*",
        "doctrine/dbal": "*",
        "laravel/framework": "^12.0",
        "laravel/fortify": "^1.25",
        "hedii/laravel-gelf-logger": "^10.0",
        "lab404/laravel-impersonate": "^1.7",
        "overtrue/laravel-versionable": "^5.4",
        "spatie/laravel-permission": "^6.16",
        "staudenmeir/laravel-adjacency-list": "^1.24",
        "laravel/socialite": "^5.18",
        "stephenlake/laravel-approval": "^1.1",
        "wotz/laravel-swagger-ui": "^1.2",
        "elasticsearch/elasticsearch": "^8.17",
        "theodo-group/llphant": "^0.11.1",
        "laravel/horizon": "^5.31",
        "awobaz/compoships": "^2.4",
        "babenkoivan/elastic-scout-driver": "^4.0",
        "typesense/typesense-php": "^5.0",
        "laravel/scout": "^10.14",
        "matanyadaev/laravel-eloquent-spatial": "^4.5",
        "babenkoivan/elastic-scout-driver-plus": "^5.1",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/laravel-medialibrary": "^11.12"
    },
    "require-dev": {
        "mtrajano/laravel-swagger": "^0.6.4",
        "laravel/pint": "^1.13",
        "nunomaduro/phpinsights": "^2.13",
        "peckphp/peck": "^0.1.3",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-stressless": "^3.1",
        "pestphp/pest-plugin-type-coverage": "^3.5",
        "rector/rector": "^2.0"
    },
    "scripts": {
        "lint": [
            "pint",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:models -MN",
            "php artisan ide-helper:meta"
        ],
        "check": "vendor/bin/phpstan analyse --memory-limit=2G -v",
        "fix": "vendor/bin/phpstan analyse --memory-limit=2G -v --fix",
        "refactor": "rector",
        "test:type-coverage": "pest --type-coverage --exactly=100",
        "test:typos": "peck",
        "test:lint": "pint --test",
        "test:unit": "pest --parallel --coverage --exactly=100",
        "test:types": "phpstan",
        "test:refactor": "rector",
        "test": [
            "@test:type-coverage",
            "@test:typos",
            "@test:unit",
            "@test:lint",
            "@test:types",
            "@test:refactor"
        ],
        "version": "./scripts/version.sh --nointeractive",
        "version:silent": "./scripts/version.sh --silent --nointeractive",
        "version:major": "./scripts/version.sh major",
        "version:minor": "./scripts/version.sh minor",
        "version:patch": "./scripts/version.sh patch",
        "setup:hooks": "./scripts/setup-hooks.sh"
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {

            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Core\\": "app/",
            "Modules\\Core\\Database\\Factories\\": "database/factories/",
            "Modules\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Core\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
